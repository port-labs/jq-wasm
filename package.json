{
    "name": "@port-labs/jq-wasm",
    "version": "0.0.1",
    "description": "WASM bindings for JQ",
    "jq-wasm": "0.0.1",
    "main": "dist/jq.browser.js",
    "scripts": {
        "build:dev": "bash -c '. /opt/emsdk/emsdk_env.sh && make browser'",
        "build:full": "bash -c '. /opt/emsdk/emsdk_env.sh && make build_deps && make browser'",
        "install": "bash -c 'npm run build:full'",
        "start": "pushd test_server && node index"
    },
    "keywords": [
        "jq",
        "binding",
        "wasm"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/port-labs/jq-wasm.git"
    },
    "license": "MIT",
    "engines": {
        "node": ">=6.0.0"
    }
}